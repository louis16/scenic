.container {
	height: 100vh;
	overflow: hidden;
}

.logoView {
	display: flex;
	flex-direction: row;
	align-items: center;
}

.logoView>image {
	width: 48rpx;
	height: 48rpx;
	background-color: rebeccapurple;
	margin-right: 12rpx;
}

.logoView>text {
	font-family: 'PingFang SC Bold';
	font-size: 34rpx;
	font-weight: 400;
	line-height: 34rpx;
	text-align: center;
	color: #ffffff;
}

.searchIcon {
	width: 44rpx;
	height: 44rpx;
	margin-right: 30rpx;
}