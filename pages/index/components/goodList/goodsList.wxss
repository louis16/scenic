.good-list-wrap {
  padding: 44rpx 34rpx 400rpx 34rpx;
  display: grid;
  grid-template-columns: 120rpx 120rpx 120rpx 120rpx 120rpx;
  grid-column-gap: 24rpx;
  grid-row-gap: 32rpx;
}

.good-empty {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  top: 0;
}

.good-empty-icon {
  width: 480rpx;
  height: 360rpx;
  transform: scale(.8);
  transform: translateY(-60%);
}

.good-item-wrap {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  background-color: #90ADD8;
  border-radius: 0 20rpx 0 20rpx;
  padding: 4rpx;
  box-sizing: border-box;
}

.good-item {
  position: relative;
  width: 100%;
  height: 100%;
  border: 1rpx solid white;
  border-radius: 0 16rpx 0 16rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.good-item::after {
  content: ' ';
  position: absolute;
  left: -1rpx;
  top: -1rpx;
  width: 100%;
  height: 100%;
  border: 1rpx solid white;
  box-sizing: content-box;
  border-radius: 16rpx 0 16rpx 0;
}


.good-item-num {
  position: absolute;
  z-index: 99;
  right: 2rpx;
  font-family: 'DIN-Bold';
  font-size: 26rpx;
  line-height: 26rpx;
  -webkit-text-stroke-width: 1rpx;
  -webkit-text-stroke-color: #567DB5;
  -webkit-text-stroke: 1rpx #567DB5;
  color: #fff;
}

.good-item-img {
  width: 80rpx;
  height: 80rpx;
  position: relative;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.good-item-img-net{
  width: 60rpx;
  height: 60rpx;
  border: 1rpx solid white;
  border-radius: 16rpx;
}

.good-composition-mask {
  position: absolute;
  z-index: 99;
  height: 32rpx;
  width: 100%;
  bottom: 0;
  background: linear-gradient(269deg, rgba(0, 0, 0, 0.10) 0%, rgba(14, 14, 14, 0.60) 28%, rgba(28, 28, 28, 0.60) 71%, rgba(84, 84, 84, 0.10) 99%);
  color: white;
  text-align: center;
  font-size: 20rpx;
  line-height: 32rpx;
}