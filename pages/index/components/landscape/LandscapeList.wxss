.landscapeListWrap {
  height: 100%;
  background: linear-gradient(180deg, #FFFFFF 0%, #EFF5FF 100%);
  overflow-y: auto;
  padding: 24rpx 30rpx 350rpx 30rpx;
  display: grid;
  grid-template-columns: 332rpx 332rpx;
  grid-column-gap: 26rpx;
  grid-template-rows: 452rpx;
  grid-row-gap: 32rpx;
}

.landscapeItem {
  width: 332rpx;
  height: 400rpx;
  background-color: #D5E1F2;
  border-radius: 32rpx;
  padding: 16rpx;
  box-sizing: border-box;
  box-shadow: 0px 0px 5px 0px #D5E1F2;
}

.img {
  width: 300rpx;
  height: 300rpx;
  border-radius: 28rpx;
}

.title {
  font-family: 'PingFang SC Bold';
  font-size: 32rpx;
  font-weight: 600;
  line-height: 44rpx;
  text-align: center;
  margin-top: 16rpx ;
}