.mask_layer {
  width: 100vw;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  position: relative;
  z-index: 100;
  display: flex;
  align-items: center;
  justify-content: center;
}
.slideUp{
  transform: translateY(100%);
}
.fadeout {
  transform: scale(0.5);
  opacity: 0;
}
.align-center{
  width: 100%;
  text-align: center;
}
.font_style_1 {
  font-family: JinNanJunJunTi-Bold;
  font-weight: normal;
  text-align: center;
  letter-spacing: 0em;
  color: #FFF09E;
}
.font_style_2{
  font-family: PingFang SC Regular;
  font-weight: normal;
  letter-spacing: 0em;
  color: #fff;
}
.border-0{
  border: 0!important;
}
.fs-20{
  font-size: 20rpx;
}
.fs-24{
  font-size: 24rpx;
}
.fs-28{
  font-size: 28rpx;
}
.fs-32{
  font-size: 32rpx;
}
.fs-40{
  font-size: 40rpx;
}
.mgt-0{
  margin-top: 0!important;
}
.flex_nowrap {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
}
.flex_wrap {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
/* 初始化状态 */
.default_wrap {
  position: absolute;
  bottom: 0;
  width: 100vw;
  height: 376rpx;
}

.default_left {
  width: 262rpx;
  height: 376rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 3;
}

.default_right {
  width: 262rpx;
  height: 376rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 1;
}

.default_middle {
  width: 226rpx;
  height: 260rpx;
  position: absolute;
  left: 50%;
  top: 0;
  margin-left: -113rpx;
}

/* 任务 */
.task_bottom_wrap {
  position: absolute;
  bottom: 0;
  width: 100vw;
  height: 676rpx;
}

.task_left {
  width: 362rpx;
  height: 366rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 10;
}

.task_middle {
  background-size: 100% 100%;
  width: 636rpx;
  height: 612rpx;
  position: absolute;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  z-index: 9;
}

.task_content {
  padding: 64rpx 56rpx;
}

.task_title_wrap {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  position: relative;
  margin-bottom: 44rpx;
}

.liner_line::after {
  content: ' ';
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  width: 90%;
  height: 4px;
  background: linear-gradient(270deg, rgba(0, 0, 0, 0.00) 0%, #472419 54%, rgba(0, 0, 0, 0.00) 100%);
  bottom: -20rpx;
}

.task_title {
  flex: 1;
  text-align: center;
  font-family: 'JinNanJunJunTi-Bold';
  font-weight: normal;
  text-align: center;
  letter-spacing: 0em;
  color: #FFF09E;
  background-clip: text;
  -webkit-background-clip: text;
  color: transparent;
  background-position: center;
  background-repeat: no-repeat;
}

.title_desc {
  font-family: 'PingFang SC Bold';
  font-weight: normal;
  letter-spacing: 0em;
  color: #2F1407;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.task_box {
  text-align: center;
  width: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.task_box image {
  margin-bottom: 10rpx;
}

.task_icon {
  width: 120rpx;
  height: 120rpx;
}

.task_audio_icon {
  width: 40rpx;
  height: 40rpx;
}

.task_audio_icon::after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  transform: translate(-25%, -25%);
  width: 70rpx;
  height: 70rpx;
}

.task_middle .btn_close {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: -15rpx;
  top: -15rpx;
}

.task_right {
  width: 262rpx;
  height: 328rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 8;
}

.task_old_man {
  width: 348rpx;
  height: 346rpx;
  position: absolute;
  right: -15rpx;
  bottom: 0;
  z-index: 10;
}

.task_old_man.left {
  left: -15px;
  transform: scaleX(-1);
}

