/*对话框*/
.dialogBox {
  position: absolute;
  bottom: 0;
  width: 100vw;
  height: 416rpx;
  display: flex;
  justify-content: center;
}

.dialogBox .dialog_middle {
  position: relative;
  z-index: 2;
  width: 710rpx;
  height: 316rpx;
  background-repeat: no-repeat;
  background-size: cover;
}
.dialogBox .dialog_middle .task_audio_icon{
  position: absolute;
  right: 130rpx;
  top:48rpx;
}
.dialogBox .dialog_middle>view {
  box-sizing: content-box;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding: 56rpx 66rpx;
}

.dialogBox .dialog_middle>view>view:first-child {
  font-family: JinNanJunJunTi-Bold;
  font-size: 32rpx;
  font-weight: normal;
  letter-spacing: 0em;
  color: #FFF09E;
  height: 32rpx;
  margin-bottom: 30rpx;
}

.dialogBox .dialog_middle.left>view>view:first-child {
  padding-left: 200rpx;
}

.dialogBox .dialog_middle>view>scroll-view {
  font-family: 'PingFang SC Bold';
  padding-left: 20rpx;
  padding-right: 200rpx;
  font-size: 28rpx;
  font-weight: normal;
  letter-spacing: 0em;
  color: #2F1407;
  width: auto;
  height: 140rpx;
}

.dialogBox .dialog_middle.left>view>scroll-view {
  padding-left: 200rpx;
  padding-right: 20rpx;
}

.dialogBox .mysteryMan::before {
  content: '?';
  display: inline-block;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -30rpx;
  margin-top: -80rpx;
  font-family: JinNanJunJunTi-Bold;
  font-size: 102rpx;
  font-weight: normal;
  text-align: center;
  letter-spacing: 0em;
  color: #FFF09E;
}

.dialogBox .mysteryMan.left::before {
  transform: scaleX(-1);
}

.dialogBox .next_btn {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 32rpx;
  top: 28rpx;
}

/*背包*/
.bag {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.bag .bag_title {
  position: relative;
  transform: translateY(-100%);
}

.bag .bag_title text {
  position: absolute;
  bottom: 30rpx;
  left: 100rpx;
  font-family: JinNanJunJunTi-Bold;
  font-size: 40rpx;
  font-weight: normal;
  text-align: center;
  letter-spacing: 0em;
  color: #FFFFFF;
  text-shadow: 0px 1px 2px 0px #353A08;
}

.bag .bag_title_bg {
  width: 276rpx;
  height: 136rpx;
}

.bg_cover,
.bg_contain {
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

.bg_contain {
  background-size: contain;
}

.bag .bag_box {
  box-sizing: border-box;
  width: 100vw;
  flex: 1;
  transform: translateY(100%);
  background-position: center top;
  background-size: 100% calc(100% - 60rpx);
  padding: 70rpx;
}
.bag .bag_box .bag_box_content{
  height: 830rpx;
}
.bag .bag_box .left_bg{
  position: absolute;
  left:0;
  bottom: 0;
  width: 350rpx;
  height:346rpx;
  z-index: 2;
}
.bag .bag_box .right_bg1{
  position: absolute;
  right:0;
  bottom: 0;
  width: 300rpx;
  height: 240rpx;
  z-index: 2;
}
.bag .bag_box .right_bg2{
  position: absolute;
  right: -230rpx;
  bottom: 0;
  width: 518rpx;
  height: 518rpx;
  z-index: 1;
}
.bag .bag_box .btn_close {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: -10rpx;
  top: -15rpx;
}
.bag .bag_box .tip_box{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bag .bag_box .tip_box .tip{
  margin-top: -200rpx;
  width: 610rpx;
  min-height: 256rpx;
  border-radius: 28rpx;
  opacity: 1;
  background: rgba(0, 0, 0, 0.75);
  box-sizing: border-box;
  /* #FFF09E */
  border: 2rpx solid #FFF09E;
  padding: 26rpx;
}
.bag .bag_box .tip_box .tip .header {
  display: flex;
  flex-direction: row;
  align-items: stretch;
  border-bottom: 1px solid rgba(255, 255, 255, 0.2);
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
}
.bag .bag_box .tip_box .tip .header view {
  margin-left: 8rpx;
}
.bag .bag_box .tip_box .tip .header>view:last-child {
  flex: 1;
  font-size: 0;
  padding: 12rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.bag .bag_box .tip_box .tip .header view>view:first-child {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.bag .bag_box .tip_box .tip .main {
  border-bottom: 1px solid rgba(255, 255, 255, 0.2);
  padding-bottom: 20rpx;
}
.bag .bag_box .tip_box .tip .main .number_2 {
  width: 530rpx;
  height: 265rpx;
}
.bag .bag_box .tip_box .tip .main .number_4,
.bag .bag_box .tip_box .tip .main .number_8 {
  width: 530rpx;
  height: 530rpx;
}
.bag .bag_box .tip_box .tip .main .number_6 {
  width: 530rpx;
  height: 352rpx;
}
.bag .bag_box .tip_box .tip .main .image_map {
  position: relative;
}
.bag .bag_box .tip_box .tip .main .image_map .lock_mask{
  position: absolute;
  top:0;
  left:0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex-wrap: wrap;
  align-content: stretch;
}
.bag .bag_box .tip_box .tip .main .image_map .lock_mask view.number_2,
.bag .bag_box .tip_box .tip .main .image_map .lock_mask view.number_4{
  width: 265rpx;
  height: 265rpx;
}
.bag .bag_box .tip_box .tip .main .image_map .lock_mask view.number_6,
.bag .bag_box .tip_box .tip .main .image_map .lock_mask view.number_8{
  width: 176rpx;
  height: 176rpx;
}
.bag .bag_box .tip_box .tip .main .image_map .lock_mask view.unlock {
  background-color: rgba(0, 0, 0, 0.5);
}
.bag .bag_box .tip_box .tip .main .image_map .lock_mask view.unlock::before {
  content: '🔒'; /*后面换成服务器图片地址*/
  display: flex;
  height: 100%;
  align-items: center;
  justify-content: center;
}
.bag .bag_box .tip_box .tip .main .detail text:first-child{
  margin: 34rpx 0 24rpx 0;
}
.bag .bag_box .tip_box .tip .footer {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
}
.bag .bag_box .tip_box .tip .footer text{
  color:#FFF09E;
}
.bag .pic_box {
