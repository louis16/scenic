/* pages/user/user.wxss */
@import "./style/users.wxss";
.container{
  position: relative;
  height: 100vh;
  overflow: hidden;
}

.user-bg{
  position: fixed;
  z-index: -999;
  left: 0px;
  top: 0px;
  height: 100%;
  width: 100%;
}
.userinfo{
    padding: 190rpx 0rpx 0rpx 24rpx;
    display: flex;
}
.userinfo-head{
  width: 128rpx;
  height: 128rpx;
  position: relative;
}
.userinfo-head::before{
  content: '';
  position: absolute;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 120rpx;
  height: 62rpx;
  left: 4rpx;
  bottom: -40rpx;
  z-index: -1;
}
.userinfo .userinfo-head image{
  border-radius: 50%;
  border:solid 6rpx white;
}
.userinfo-content{
  padding-top: 18rpx;
  padding-left: 32rpx;
  font-size: 40rpx;
}
.userinfo-content .uname{
  font-family: 'PingFang SC Heavy';
}
.userinfo-content .uinfo{
  display: flex;
}
.userinfo-content .uother{
  margin-top: 32rpx;
  height: 32rpx;
  margin-left: 6rpx;
  position: relative;
  padding-left: 42rpx;
  padding-right: 12rpx;
  line-height: 20rpx;
  border-radius: 20rpx;
}
.userinfo-content .umale{
  background: #ADC0DE;
}
.userinfo-content .ufemale{
  background: #FFC7C7;
}
.userinfo-content .uother .uother_sex{
  width: 36rpx;
  height: 36rpx;
  left: -2rpx;
  top: -2rpx;
  position: absolute;
}
.userinfo-content .uother .uother_age{
  font-size: 24rpx;
  color: white;
}
.uinfo .uother_edit{
  padding-top: 14rpx;
}
.uinfo .uother_edit image{
  padding-right: 6rpx;
  width: 20rpx;
  height: 20rpx;
}
.uinfo .uother_edit text{
  font-size: 24rpx;
  color: #333333;
}
.uhonour{
  display: flex;
  margin:22rpx 0rpx 0rpx 18rpx;
  position: relative;
  padding-right: 40rpx;
}
.uhonour_item{
  margin-right: 6rpx;
  width: 32rpx;
  height: 32rpx;
}
.userinfo-integral{
  display: flex;
  height: 132rpx;
  margin-top: 60rpx;

}
.userinfo-integral > view{
  width: 50%;
  text-align: center;
}
.userinfo-integral > view > view{
  color: #333333;
  padding-top: 18rpx;
  padding-bottom: 4rpx;
  font-size: 40rpx;
  font-weight: bold;
  position: relative;
}
.userinfo-integral > view > view::before{
  position: absolute;
  content: '';
  left: 0px;
  top:46rpx;
  height: 40rpx;
  width:2rpx;
  background-color: #CCCCCC;
}
.userinfo-integral > view > text{
  color: #666666;
  font-size: 28rpx;
}
.user-nav{
  width: 650rpx;
  border-radius: 16rpx;
  box-shadow: 4rpx 4rpx 10rpx #ccc;
  margin-left: 30rpx;
  margin-top: 40rpx;
  padding:0px 20rpx;
}
.unav-item{
  height: 84rpx;
  padding-top: 24rpx;
  display: flex;
  border-bottom: solid 2rpx #F1F2F6;
}
.unav-item:last-child{
  border-bottom:none;
}
.unav-item .unav-item__titel{
  width: 194rpx;
  line-height: 50rpx;
  padding-left: 14rpx;
}
.unav-item .unav-item__value{
  width: 340rpx;
  text-align: right;
  line-height: 55rpx;
  padding-left: 14rpx;
  color: #999999;
}
.unav-item image{
  width: 56rpx;
  height: 56rpx;
}
.unav-item::before{
  top:44rpx!important;
}
.unav-item{
  font-size:28rpx;
  color: #333333;
}
.uedit-title{
  text-align: center;
  font-size: 40rpx;
  padding-top: 80rpx;
}
.uedit-sxe{
  width: 590rpx;
  display: flex;
  margin-left: 80rpx;
  margin-top:56rpx;
  border-bottom: solid 2rpx #ececec;
  padding-bottom: 70rpx;
}

.uedit-sxe > view{
  width: 144rpx;
  text-align: center;
  width: 50%;
}
.uedit-sxe > view > view{
  margin-left: 75rpx;
  width: 144rpx;
  height: 144rpx;
  background-color:#FBC3C3;
  margin-bottom: 14rpx;
  border-radius: 114rpx;
  position:relative;
}
.uedit-sxe image.uedit-sxe__head{
  margin-left:0rpx ;
  margin-top: 46rpx;
  width: 80rpx;
  height: 98rpx; 
}
.uedit-sxe .uedit-sxe__select{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  bottom: 0rpx;
  right: 22rpx;
}
.imgfilter{
    background-color: #D8D8D8!important;
}
.imgfilter .uedit-sxe__select{
  display: none;
}
.imgfilter image{
  -webkit-filter: grayscale(100%);
  filter: grayscale(100%);
  filter: gray;
}
.udeid-date .picker{
  width: 402rpx;
  height: 72rpx;
  margin-top: 56rpx;
  margin-left: 156rpx;
  border: solid 2rpx #ECECEC;
  border-radius: 36rpx;
  line-height: 72rpx;
  padding-left: 36rpx;
  font-size: 28rpx;
  color: #999999;
}
.udeid-date .picker::before{
  right: 30rpx;
  top: 26rpx;
}
.uedid-button{
  margin-top: 130rpx;
  padding: 16rpx;
  border-top: solid 2rpx #ECECEC;
  border-bottom: solid 2rpx #ECECEC;
}
.uedid-button button{
  width: 410rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: white;
  font-weight: normal;
  padding: 0rpx;
  border-radius: 28rpx;
  background: -webkit-linear-gradient(top,#C5DAF8,#668FCC);
}

