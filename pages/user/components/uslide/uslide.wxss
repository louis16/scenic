/* pages/user/components/uslide/uslide.wxss */
@import "../../style/users.wxss";
.uslidecontainer{
  position: fixed;
  z-index: 10001;
  background-color:rgba(0,0,0, 0.5);
  width: 100%;
  height: 100%;
  left: 0px;
  top: 0px;
}
.uslide-main{
  width: 100%;
  position: absolute;
  padding-bottom: 68rpx;
  border-top-left-radius: 48rpx;
  border-top-right-radius: 48rpx;
  background: white;
  min-height: 120rpx;
  left: 0rpx;
  bottom: 0rpx;
}