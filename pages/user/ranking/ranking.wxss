/* pages/user/ranking/ranking.wxss */
@import "../style/users.wxss";
.linearBg{
  min-height: 100vh;
  position: relative;
}
.linearBg::before{
  content: '';
  width: 100%;
  height: 612rpx;
  position: absolute;
  z-index: -1;
  left: 0rpx;
  top: 0rpx;
  /* background: linear-gradient(360deg, rgba(255,255,255,0.00) 0%, #FF6B2C 99%); */
  background: url(data:image/png;base64,) repeat-x;
}
.ranking{
  width: 690rpx;
  margin-left: 30rpx;
  padding-bottom: 150rpx;
}
.ranking-notice{
  text-align: center;
  margin-top: 24rpx;
}
.ranking-notice view{
  display: inline-block;
  height: 60rpx;
  overflow: hidden;
  line-height: 60rpx;
  border-radius: 34rpx;
  background: #FF9162;
  color: white;
  font-size: 32rpx;
  padding: 0rpx 30rpx;
  font-family: PingFang SC Regular;
}
.rankingnav{
  /* width: 750rpx; */
  width: 720rpx;
  margin-left: 30rpx;
  overflow: hidden;
  height: 64rpx;

  /* overflow: hidden; */
  margin-top: 50rpx;
  position: relative;
}
.rankingmain{
  width: 690rpx;
  height: 64rpx;
  overflow: hidden;
}
.rankingnav_items{
  position: absolute;
  /* width: 10000rpx; */
  display: flex;
  /* position: relative; */
  padding-bottom: 14rpx;
  flex-wrap: nowrap;
  /* padding-top: 50rpx; */
  line-height: 64rpx;
  z-index: 1;
  left: 0rpx;
  top: 0rpx;
}
.rankingnav_items view{
  width: 172.5rpx;
  /* width: 200rpx; */
  text-align: center;
  font-size: 28rpx;
  overflow: hidden;
  color: #333333;
}
.rankingnav_items view text{
  display: inline-block;
  position: relative;
  /* padding-bottom: 18rpx; */
}
.rankingnav_items view text::before{
  content: "";
  position: absolute;
  width: 100%;
  height:8rpx ;
  background: #FF6B2C;
  bottom: 0rpx;
  left: 0rpx;
  display: none;
  border-radius: 4rpx;
}

.rankingnav_items view.active{
  color: #FF6B2C;
}
.rankingnav_items view.active text::before{
  display: block;
}
.rankingnav_items .nav-line{
  content: "";
  position: absolute;
  /* width: 100%; */
  width: 172.5rpx;
  height:8rpx ;
  background: #FF6B2C;
  bottom: 0rpx;
  left: 0rpx;
  border-radius: 4rpx;
}
.rankingnav-main{
  padding-top: 24rpx;
}
.navmoreBut{
  position: absolute;
  width: 80rpx;
  height: 64rpx;
  top: 0rpx;
  right: 0rpx;
  text-align: center;
  z-index: 4;
  /* background: linear-gradient(180deg, #FDD6C5 0%, #FCE3D9 100%); */
  /* background: linear-gradient(180deg, #ffd7c7 0%, #ffe4d9 100%); */
  background: linear-gradient(180deg, #ffddcf 0%, #ffeae1 100%);
}
.navmoreBut > image{
  /* width: 80rpx;
  height: 64rpx; */
  width: 33rpx;
  height: 25rpx;
  margin: 19.5rpx;
  position: relative;
  z-index: 1;
}
